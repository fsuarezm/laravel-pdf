{
    "name": "fsuarezm/laravel-pdf",
    "description": "Rospdf for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Francisco SuÃ¡rez",
            "email": "fsuarezm@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pdf\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pdf\\Laravel\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "rospdf/pdf-php": "^0.12",
        "illuminate/support": "^5.7",
        "illuminate/config": "^5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "archive": {
        "exclude": [
            "/tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pdf\\Laravel\\PdfServiceProvider"
            ],
            "aliases": {
                "Pdf": "Pdf\\Laravel\\Facades\\Pdf"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
